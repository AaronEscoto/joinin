<p id="notice"><%= notice %></p>

<h1>Listing Matchmakers</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matchmakers.each do |matchmaker| %>
      <tr>
        <td><%= matchmaker.uid %></td>
        <td><%= matchmaker.address %></td>
        <td><%= matchmaker.latitude %></td>
        <td><%= matchmaker.longitude %></td>
        <td><%= link_to 'Show', matchmaker %></td>
        <td><%= link_to 'Edit', edit_matchmaker_path(matchmaker) %></td>
        <td><%= link_to 'Destroy', matchmaker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matchmaker', new_matchmaker_path %>
